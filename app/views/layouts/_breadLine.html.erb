<% content_for :breadLine do %>
  <div class="breadLine">
    <div class="bc">
      <% if content_for?(:breadcrumbs) %>
        <%= content_for(:breadcrumbs) %>
      <% else %>
        <ul id="breadcrumbs" class="breadcrumbs">
          <li><%= link_to (content_for?(:pageTitle) ? content_for(:pageTitle) : "Dashboard"), root_path %></li>
          <li class="current"><%= link_to (content_for?(:pageTitle) ? content_for(:pageSubTitle) : "Unknown"), "#unknown" %></li>
        </ul>
      <% end %>
    </div>
  </div>
<% end %>
